<?xml version="1.0" encoding="utf-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android"
    >

    <item
        android:width ="50dp"
        android:height="50dp">
        <shape
            android:shape="oval"
            >
            <solid
                android:color="@color/colorPrimary"
                />
            <size
                android:width="50dp"
                android:height="50dp"
                />
            <stroke
                android:width="0.5dp"
                />
        </shape>
    </item>

    <item
        android:height="25dp"
        android:width="25dp"
        android:gravity="center"
        android:drawable="@drawable/add_sign">
    </item>


</layer-list>